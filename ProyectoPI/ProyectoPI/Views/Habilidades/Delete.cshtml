@model IEnumerable<ProyectoPI.Models.HABILIDADES>

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>HABILIDADES</h4>
    <hr />
    <dl class="dl-horizontal"></dl>




</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cedulaEmpleadoFK)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.valorPK)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipoPK)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cedulaEmpleadoFK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.valorPK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tipoPK)
            </td>
            <td>
                @Html.ActionLink("Delete", "DeleteConfirmed", "HABILIDADES", new { id = item.cedulaEmpleadoFK, valor = item.valorPK, tipo = item.tipoPK }, null) 

            </td>
        </tr>
    }
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()


        <div class="form-actions no-color">
            
            <input type="submit" value="Delete" class="btn btn-default" /> |
            <input type="submit" value="Regresar" class="btn btn-default" />
        </div>



    }

</table>
