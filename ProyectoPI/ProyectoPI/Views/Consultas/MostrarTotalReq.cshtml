@model List<ProyectoPI.Models.CantReq>
@{
    ViewBag.Title = "Requerimiento";
}
@{
    string rol = ViewBag.myRol;
    string idProy = ViewBag.idproy;
    string proy = ViewBag.nombre;
    List<ProyectoPI.Models.GetReq> req = ViewBag.Req;
}
<h2 class="titulosPag">Cantidad de Requerimientos por Tester</h2>
<h4>Resultados</h4>



<img src="@Url.Action("GraficoTotalReq",new { proy = idProy })" alt="Pie Chart using MVC" />
</br>
</br>

<div style=" right:3000px;width:500px ;height:auto" class="jumbotron">
    <p style="text-align:center" display-1">Resultados</p>
    <p style="font-size:15px">
    </p>
    <table class="table">
        <thead class="thead-dark">
            <tr>

                <th scope="col">Nombre del Tester</th>
                <th scope="col">Nombre del Requerimiento</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in req)
            {
                <tr>

                    <td>@item.nombre</td>
                    <td>@item.nombrePK</td>

                </tr>
            }
        </tbody>
    </table>
</div>





<div>
    @Html.ActionLink("Volver", "CantReq", null, new { @class = "botonGeneral colorGris" })
</div>
